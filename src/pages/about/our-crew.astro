---
import CTAColor from "../../components/CTAColor.astro";
import HeaderScreen from "../../components/HeaderScreen.astro";
import StaffGrid from "../../components/StaffGrid.astro";
import ourTeam from "../../data/our-team";
import BaseLayout from "../../layouts/BaseLayout.astro";
import ContentDivisor from "../../components/ContentDivisor.astro";


const { data } = ourTeam;
---

<BaseLayout
  siteTitle={data.pageTitle}
  siteDescription={data.pageDescription}
  keywords={data.pageKeywords}
>
  <HeaderScreen content={data.header} />
  <ContentDivisor content={data.staffGrid} />
  <StaffGrid data={data.staffGrid} />

  <ContentDivisor content={data.adminTeamGrid} />
  <StaffGrid data={data.adminTeamGrid} />
  
  <ContentDivisor content={data.flightTeamGrid} />
  <StaffGrid data={data.flightTeamGrid} />
  
  <ContentDivisor content={data.maintenanceTeamGrid} />
  <StaffGrid data={data.maintenanceTeamGrid} />
  
  <CTAColor content={data.flyWithUsCTA} />
</BaseLayout>
